
{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "ESNext"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": false,
    "skipLibCheck": true,
    "baseUrl": "./",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
